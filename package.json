{
  "name": "aura-bank---integrated-fintech-ecosystem",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:frontend": "vite",
    "dev:backend": "cd backend && npx tsx src/index.ts",
    "dev:ml": "cd model && python ml_api.py",
    "dev:all": "concurrently -n \"API,WEB\" -c \"bgBlue,bgGreen\" \"npm run dev:backend\" \"npm run dev:frontend\"",
    "start": "concurrently -n \"API,WEB\" -c \"bgBlue,bgGreen\" \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build": "vite build",
    "build:backend": "cd backend && npm run build",
    "build:all": "npm run build && npm run build:backend",
    "preview": "vite preview",
    "install:all": "npm install && cd backend && npm install",
    "db:setup": "cd backend && psql -U postgres -d aurabank -f ../database/schema.sql",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@google/genai": "^1.38.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@types/dompurify": "^3.0.5",
    "dompurify": "^3.3.1",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "recharts": "^3.7.0",
    "three": "^0.182.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@types/qrcode.react": "^1.0.5",
    "@types/three": "^0.182.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "jsdom": "^27.4.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^4.0.18"
  }
}
